    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-fileinput/4.4.5/js/(lang).js"></script>
    <link rel="stylesheet" href="/font-awesome-4.7.0/css/font-awesome.min.css"
  </head>
  <body>
    <header>
      <nav class="navbar navbar-expand-md navbar-black fixed-top bg-white">
        <a class="navbar-brand" href="/page1">
          <img src="/images/airbnb.png" width="30" height="30" alt="">
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                Trips
              </a>
              <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                <a class="dropdown-item" href="/MyReservation">My Reservation</a>
                <a class="dropdown-item" href="#">null</a>
              </div>
            </li>
            <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                Host
              </a>
              <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                <a class="dropdown-item" href="/MyHosting">My Hosting</a>
                <a class="dropdown-item" href="/HostHome">Host a Home</a>
              </div>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#">Help</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/page7">Sign Up</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/logout">Log Out</a>
            </li>
          </ul>
        </div>
      </nav>
    </header>

    <div class="progress">
      <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%"></div>
    </div>

    <script type="text/javascript">
      {{#if fillin}}
        alert("Please Upload Image File");
      {{/if}}
    </script>

    <div class="container">
      <div class="row">

        <div class="col">
          <form action="/hosting3" method="post" enctype="multipart/form-data">
            <div class="host-description">
              <div class="hd">
                <h2>Show travelers what your space looks like</h2>
              </div>
              <div class="hd">
                <script src="/path/to/js/fileinput.js"></script>
                <script src="/path/to/themes/fa/theme.js"></script>

                <div class="file-loading">
                    <input id="input-fa" name="photo" type="file" multiple>
                </div>
                <script>
                $("#input-fa").fileinput({
                    theme: "fa",
                    uploadUrl: "/images"
                });
                </script>
                <!-- <input type="file" name="photo"> -->
              </div>
              <div>
                <span>
                  <a href="javascript:history.back()">Back</a>
                </span>
                <button type="submit" class="btn btn-primary">Submit</button>
              </div>
            </div>
          </form>
        </div>

      </div>
    </div>




    <footer class="footer">
      <div class="container">
        <span class="text-muted">&copy; Airbnb, Inc.</span>
        <span class="text-muted text-right">wowow</span>
      </div>
